<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.coors.ibikego">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".blog.BlogInsertActivity" />
        <activity android:name=".blog.BlogDetailActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".blog.BlogUpdateActivity" />
        <activity android:name=".member.MemberLoginActivity" />
        <activity android:name=".member.MemberSignUpActivity" />
        <activity android:name=".member.MemberLostPwActivity" />
        <activity android:name=".search.SearchBlogActivity"></activity>
    </application>

</manifest>